@model UDBSite.Models.Noticia

@{
    ViewBag.Title = Model.Titulo;
}
<br />
<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-default">
            <div class="panel-heading">
                <h2>@ViewBag.Title</h2>

            </div>
            <div class="panel-body " style="text-align:justify;padding:40px;line-height:22px;">
                @Model.Contenido
            </div>
            <div class="panel-footer">

                @Html.ActionLink("Regresar", "Index", "Home", null, new { @class = "btn btn-default" })
            </div>
        </div>
    </div>
</div>